
[
  {
    "name": "ecommerceContainer",
    "image": "982779134822.dkr.ecr.eu-west-1.amazonaws.com/mydjango-ecommerce",
    "cpu": 1,
    "memory": 300,
    "essential": true,
    "portMappings": [
      {
        "containerPort": 8080,
        "hostPort": 8080
      }
    ],
  "logConfiguration": {
		"logDriver": "aws",
		"options": {
			"awslogs-group": "Terraform-Ecommerce",
			"awslogs-stream-prefix": "my-ecommerce"
		}
	}
  }
]
